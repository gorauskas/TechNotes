server {
    listen 8060;
    server_name localhost;

    root   /var/www/gorauskas.org;
    index  index.html index.htm;
    access_log /var/log/nginx/gorauskas.org.access.log;
    error_log /var/log/nginx/gorauskas.org.error.log;

    location / {
        try_files $uri/ $uri.html $uri/index.html index.html =404;
    }
}
